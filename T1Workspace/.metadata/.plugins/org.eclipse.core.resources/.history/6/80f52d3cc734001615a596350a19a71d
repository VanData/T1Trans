    <xs:complexType name="DobCarrier">
        <xs:annotation>
            <xs:documentation>
                All attributes for a carrier.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date and time anything for this carrier was updated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="carrierInfo" type="dobtypes:CarrierInfo" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The carrier identifiers, names, addresses and phone numbers.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentInfo" type="dobtypes:PaymentInfo" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The payment information for this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contactInfo" type="dobtypes:ContactInfo" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The payment information for this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="services" type="dobtypes:Services" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The services that this carrier provides.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="equipment" type="dobtypes:Equipment" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The equipment that this carrier has.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="geographicCoverage" type="dobtypes:GeographicCoverage" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The geographic areas that this carrier services.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="taxInfo" type="dobtypes:TaxInfo" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The tax information for this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="insuranceAgentInfo" type="dobtypes:InsuranceAgentInfo" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The information about the carrier's insurance agents.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="documents" type="dobtypes:Document" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        The documents that this carrier provides.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="TaxInfo">
        <xs:annotation>
            <xs:documentation>
                A carrier's tax information, either W9 or W8 tax info.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date and time the tax info was updated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="w9Info" type="dobtypes:W9Info">
                    <xs:annotation>
                        <xs:documentation>
                            The W9 tax form information provided by the carrier.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="w8Info" type="dobtypes:W8Info">
                    <xs:annotation>
                        <xs:documentation>
                            The W8 tax form information provided by the carrier.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="W9Info">
        <xs:annotation>
            <xs:documentation>
                A carrier's W9 tax information.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="tin" type="types:FederalTaxIdentifier" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The federal tax id number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="type" type="dobtypes:FederalTaxIdTypes" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The type of tax id provided, SSN or EIN.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="dobtypes:FederalTaxIdStatus" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The status of the federal tax id: Pending, Valid, or NotValid.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="types:String155">
                <xs:annotation>
                    <xs:documentation>
                        The carrier name as shown on their income tax return.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="businessName" type="types:String155" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The carrier business name/disregard entity name, if different from above.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="street" type="types:String200">
                <xs:annotation>
                    <xs:documentation>
                        The street address.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="city" type="types:String50">
                <xs:annotation>
                    <xs:documentation>
                        The city address.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="stateProvince" type="types:StateProvince">
                <xs:annotation>
                    <xs:documentation>
                        The State or Province.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="postalCode" type="types:PostalCodePattern">
                <xs:annotation>
                    <xs:documentation>
                        The Postal code.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="federalTaxClassification" type="dobtypes:FederalTaxClassification">
                <xs:annotation>
                    <xs:documentation>
                        The W9 Fedearal Tax Classification.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="W8Info">
        <xs:annotation>
            <xs:documentation>
                A carrier's W8 tax information.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="types:String155">
                <xs:annotation>
                    <xs:documentation>
                        The name of individual or organization that is the beneficial owner.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="typeOfBeneficialOwner" type="dobtypes:FederalTaxBeneficialOwnerTypes" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The type of beneficial owner.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="permenantAddress" type="cwtypes:Location" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The permenant address.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mailingAddress" type="cwtypes:Location" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The mailing address, if different from permenantAddress.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="foreignTaxId" type="types:String50" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The foreign tax identifying number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="foreignTaxIdType" type="types:String3" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The type of foreign tax identifying number (BN or SIN).  Only provided when dobVersion&gt;1.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="tin" type="types:FederalTaxIdentifier" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The federal tax id number if they have one.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="type" type="dobtypes:FederalTaxIdTypes" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The type of tax id provided, SSN or EIN.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
