<?xml version="1.0" encoding="UTF-8"?><!-- Best viewed with fixed-width (e.g., Courier) font --><xs:schema xmlns:dobtypes="http://www.tcore.com/TcoreDobTypes.xsd" xmlns:cwtypes="http://www.tcore.com/TcoreCarrierWatchTypes.xsd" xmlns:types="http://www.tcore.com/TcoreTypes.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.tcore.com/TcoreDobTypes.xsd" elementFormDefault="qualified">

    <xs:import namespace="http://www.tcore.com/TcoreTypes.xsd" schemaLocation="TcoreTypes.xsd"/>
    <xs:import namespace="http://www.tcore.com/TcoreCarrierWatchTypes.xsd" schemaLocation="TcoreCarrierWatchTypes.xsd"/>
    <xs:annotation>
        <xs:documentation>
            <h2>TransCore DAT-On-Boarding XML Schema</h2>
            <br/>
            This schema defines the types used by DAT On-Boarding.  
            <br/>
            Published:  April, 2012<br/>
            Version  :  1.0.0<br/>
