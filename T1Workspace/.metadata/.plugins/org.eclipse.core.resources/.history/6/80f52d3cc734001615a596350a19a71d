    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="CarrierInfo">
        <xs:annotation>
            <xs:documentation>
                The general identifying attributes of a carrier.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date and time any of the data in this section was updated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalName" type="types:String155" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The legal name that this carrier operates under.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dbaName" type="types:String155" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The Doing-Business-AS name of the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dotNumber" type="types:NonNegativeInt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The FMCSA DOT number assigned to this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="docket" type="cwtypes:DocketType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The FMCSA MC/MX/FF number assigned to this carrier.
                        <br/>
                        DOT docket is made up of a prefix and a number.
                        Prefix is a 2-character String.
                        The number is a non-negative 32-bit integer. 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="intrastate" type="cwtypes:IntrastateType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The intrastate code given to this carrier.  Only provided if the carrier is only a 
                        intrastate carrier.
                        <br/>
                        The intrastate type is made up of 
                        1) The state the carrier is authorized to operate within. A 2-character string.
                        2) The state's assigned operating code for this carrier. A 15-digit number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="duns" type="dobtypes:DUNS" minOccurs="0" maxOccurs="1">
            <xs:annotation>
                    <xs:documentation>
                        The 9 digit duns number for this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="scacCode" type="types:String4" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        The SCAC codes assigned to this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="website" type="xs:anyURI" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Company website URL, max 256 characters.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="physicalAddress" type="cwtypes:Location" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The physical address and phone numbers of this company.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mailingAddress" type="cwtypes:Location" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The mailing address and phone numbers of this company.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="ContactInfo">
        <xs:annotation>
            <xs:documentation>
                The payment information for a carrier.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date and time any of the contacts were updated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contacts" type="dobtypes:Contact" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        List of personnel and their roles in the company.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="PaymentInfo">
        <xs:annotation>
            <xs:documentation>
                The payment information for a carrier.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date and time any of the data in this section was updated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="remitToName" type="types:String155" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The name to make payments out to.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="remitToAddress" type="cwtypes:Location">
                <xs:annotation>
                    <xs:documentation>
                        The address to send payments.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="isFactoringCompany" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not the remit-to name and address are for a factoring company.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="interestInQuickPay" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates interest in quick-pay plans.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="references" type="dobtypes:Reference" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Payment references that may be contacted.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="Services">
        <xs:annotation>
            <xs:documentation>
                The services that the carrier provides and organizations that the carrier is a member.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdate" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date and time any of the data in this section was updated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companyType" type="dobtypes:CompanyTypes" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The type of company this is.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="yearFounded" type="types:Year" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The year the company was founded.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="womanOwned" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates if the carrier is owned by a woman.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="veteranOwned" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates if the carrier is owned by a military veteran.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="disabledVeteranOwned" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates if the carrier is owned by a disabled military veteran.  
                        Only provided when dobVersion&gt;1 is specified.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="minorityOwnership" type="dobtypes:MinorityOwnership" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates if the carrier is owned by a minority.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
