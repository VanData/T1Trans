    <xs:complexType name="DobEvent">
        <xs:annotation>
            <xs:documentation>
                The events that occurred for a Carrier.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="carrierId" type="types:String40" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The DAT-On-Boarding identifier for the carrier that signed contracts.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="eventTypes" type="dobtypes:EventTypes" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        The types of events that were received concerning this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="signedContracts" type="dobtypes:SignedContract" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        The contracts that this carrier signed since the date provided.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:simpleType name="EventTypes">
        <xs:annotation>
            <xs:documentation>
                The possible types of events that may be returned.
                    Contract Signed, One or more contracts were signed by the carrier.
                    Profile Update, a change was made to the profile data.
                    Insurance Agent Update, a change was made to insurance agent information.
                    Tax Info Update, a change was made to the W9 or W8 tax information (whichever applies).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Contract Signed"/>
            <xs:enumeration value="Profile Update"/>
            <xs:enumeration value="Insurance Agent Update"/>
            <xs:enumeration value="Tax Info Update"/>
            <xs:enumeration value="Document Update"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="SignedCarrier">
        <xs:annotation>
            <xs:documentation>
                The carrier identifier and contracts that have been signed by them.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="carrierId" type="types:String40" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The DAT-On-Boarding identifier for the carrier that signed contracts.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="signedContracts" type="dobtypes:SignedContract" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        The contracts that this carrier signed.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="SignedContract">
        <xs:annotation>
            <xs:documentation>
                A contract and the person who electronically signed it.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contract" type="dobtypes:Contract" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The contract that was signed.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="signatory" type="dobtypes:Signatory" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The signature information for the contract.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="signed" type="xs:dateTime" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The date the contract was signed.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="url" type="xs:anyURI" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The URL from which this contract may be downloaded.  
                        Provided only when dobVersion&gt;1 and for contracts signed through AssureSign.
                        Max 256 characters.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="Contract">
        <xs:annotation>
            <xs:documentation>
                The attributes that identify a COB Customer's contract.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="types:String100" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The Name of the contract.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="description" type="types:String100" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The description of the contract.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="version" type="types:PositiveInt" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The version of the contract.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="documentStatus" type="dobtypes:DocumentStatusTypes" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The status of the contract (Active or Inactive);
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="Signatory">
        <xs:annotation>
            <xs:documentation>
                The information about the person that signed the contract.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="types:String100" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The Name of person signing a contract.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="title" type="types:String100" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The title of the person signing a contract.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
