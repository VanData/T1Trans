        <xs:complexContent>
            <xs:extension base="types:SuccessData">
                <xs:sequence>
                    <xs:element name="alarms" type="tfmi:Alarm" minOccurs="0" maxOccurs="1000"/>
                    <xs:element name="unreported" type="tfmi:AlarmId" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                In order to keep the payload size (and transmission time) 
                                reasonable, the number of alarms returned is limited. For
                                requesters with more alarms, this element contains the IDs of
                                those alarms not reported in <code>alarms</code>.<br/>
                                <br/>
                                Clients can then feed this element to a new LookupAlarmsRequest,
                                using QueryAlarmsByAlarmIds, repeating as necessary until
                                <code>unreported</code> is no longer returned.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
