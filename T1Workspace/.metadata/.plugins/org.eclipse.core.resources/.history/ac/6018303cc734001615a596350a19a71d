    <xs:complexType name="FmcsaSafetyRating">
        <xs:annotation>
            <xs:documentation>
                Safety rating(s) from FMCSA
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:sequence minOccurs="0">
                <xs:element name="date" type="xs:dateTime">
                    <xs:annotation>
                        <xs:documentation>
                            Date of the most recent FMCSA safety rating.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="rating">
                    <xs:annotation>
                        <xs:documentation>
                            Most recent FMCSA Rating.<br/>
                            <br/>
                            If this element is not included, no safety record was found.<br/>
                            <br/>
                            Note: there are a very large number of carriers that do not have a 
                            safety rating. Lack of a safety rating should not be construed as a 
                            negative mark against the carrier.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Satisfactory"/>
                            <xs:enumeration value="Conditional"/>
                            <xs:enumeration value="Unsatisfactory"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
            </xs:sequence>
            <xs:sequence minOccurs="0">
                <xs:element name="reviewType">
                    <xs:annotation>
                        <xs:documentation>
                            Indicates the type of review that resulted in the most recent rating.
                            For more detailed information regarding the reviews go to
                            http://safer.fmcsa.dot.gov/saferhelp.aspx#SafetyRating
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <!-- It appears that these two may have been combined, but are left here 
                                for backward compatibility -->
                            <xs:enumeration value="Compliance Review"/>
                            <xs:enumeration value="Safety Review"/>
                            <xs:enumeration value="CR and Security Contact Review"/>
                            <!-- Note: It's unclear whether there's a meaningful difference between these 
                                two options -->
                            <xs:enumeration value="ID/Operations"/>
                            <xs:enumeration value="ID Operations"/>
                            <xs:enumeration value="Non-Ratable"/>
                            <xs:enumeration value="Educational Contact"/>
                            <xs:enumeration value="Security Contact Review"/>
                            <xs:enumeration value="Non-Ratable"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="reviewDate" type="xs:dateTime">
                    <xs:annotation>
                        <xs:documentation>
                            The date on which the review occurred.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:sequence>
    </xs:complexType>
