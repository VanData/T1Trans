    <xs:complexType name="DotHeader">
        <xs:sequence>
            <xs:element name="dotNumber" type="types:NonNegativeInt">
                <xs:annotation>
                    <xs:documentation>
                        DOT Identification Number of the carrier. If no DOT number is available, 
                        0 will be returned.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="docketNumber" type="types:NonNegativeInt">
                <xs:annotation>
                    <xs:documentation>
                        Docket Number of the carrier (MC, MX, or FF). If no Docket number is
                        available, 0 will be returned.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dbaName" type="types:String155" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Commonly used business name.<br/>
                        <br/>
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalName" type="types:String155" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Legally registered business name.<br/>
                        <br/>
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="address" type="types:Address" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Address registered with the DOT. Not included if DOT registration
                        is no longer active.<br/>
                        <br/>
                        Note that there are instances of incorrect registration data, in
                        which the postal code is not of the proper format for the designated
                        country code.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="phone" type="types:PhoneNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Phone number registered with the DOT.<br/>
                        <br/>
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="fax" type="types:PhoneNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Fax number registered with the DOT.<br/>
                        <br/>
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mailingAddress" type="types:Address" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Mailing address registered with the DOT.<br/>
                        <br/>
                        Not included if DOT registration is no longer active.<br/>
                        <br/>
                        Note that there are instances of incorrect registration data, in
                        which the postal code is not of the proper format for the designated
                        country code.
                    </xs:documentation>
