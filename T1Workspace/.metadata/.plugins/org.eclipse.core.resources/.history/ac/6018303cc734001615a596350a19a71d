                            <xs:documentation>
                                Carrier's state.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="carrierZipCode" type="types:PostalCodePattern">
                        <xs:annotation>
                            <xs:documentation>
                                Carrier's zip/postal code.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="carrierDotNumber" type="xs:nonNegativeInteger" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Carrier's DOT number.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="carrierDocketNumber" type="cwtypes:DocketType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Please enter at least one of the following, so TransCore can 
                                identify the carrier:
                                <ul>
                                <li>carrierMcNumber</li>
                                <li>carrierIntrastateCode</li>
                                <li>carrierDotNumber.</li>
                                </ul>
                            </xs:documentation>
                        </xs:annotation>
