    <xs:complexType name="DeleteSearchRequest">
        <xs:annotation>
            <xs:documentation>
                Delete a search.<br/>
                <br/>
                It is not a requirement that searches ever be explicitly deleted. Deletion is a
                convenience for applications that make heavy use of multi-search lookups (e.g.,
                <code>LookupSearchOperation.queryAllMySearches</code>) so they can easily be
                excluded from lookups.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="message:Request">
                <xs:sequence>
                    <xs:element ref="tfmi:deleteSearchOperation"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
