        <xs:complexContent>
            <xs:extension base="types:Operation">
                <xs:sequence>
                    <xs:element name="criteria" type="tfmi:SearchCriteria">
                        <xs:annotation>
                            <xs:documentation>
                                The criteria to be used for this search.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="sortOrder" type="tfmi:SortOrder" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                The sort order to apply to the search results. The maximum number
                                of matches returned depends on the requesting customer's 
                                subscriptions, configuration, and TransCore policies.<br/>
                                <br/>
                                If omitted, defaults to SortOrder.AGE (i.e., newest matching
                                assets first).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="includeSearch" type="xs:boolean" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                If set true, then the response will return a copy of the search
                                as it was actually processed by the Freight Matching Service.
                                This saves the requester from doing a subsequent LookupSearch
                                request.<br/>
                                <br/>
                                If omitted, defaults to false.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="createSearchOperation" type="tfmi:CreateSearchOperation" substitutionGroup="types:operations"/>
    <!-- -->
    <!-- -->
    <!-- ======================================================================================= -->
    <!-- -->
    <!-- -->
