    <xs:complexType name="InsuranceDescription">
        <xs:annotation>
            <xs:documentation>
                Describes an insurance policy carried by the
                carrier.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="bipdPolicy" type="types:String90" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The policy identified for this insurance coverage
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="effectiveDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>
                        Date this insurance policy became effective.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="canceledDate" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Date this insurance policy was canceled.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="formCode">
                <xs:annotation>
                    <xs:documentation>
                        <ul>
                            <li>34 = Cargo</li>
                            <li>91,91X = BIPD</li>
                            <li>82 = BIPD (Surety Bond)</li>
                            <li>83 = Cargo (Surety Bond)</li>
                            <li>84 = Property Brokers Surety Bond</li>
                            <li>85 = Property Brokers Trust Fund Agreement</li>
                        </ul>
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="34"/> <!-- Cargo -->
                        <xs:enumeration value="82"/> <!-- BIPD (Surety Bond) -->
                        <xs:enumeration value="83"/> <!-- Cargo (Surety Bond) -->
                        <xs:enumeration value="84"/> <!-- Property Brokers Surety Bond -->
                        <xs:enumeration value="85"/> <!-- Property Brokers Trust Fund Agreement -->
                        <xs:enumeration value="91"/> <!-- BIPD -->
                        <xs:enumeration value="91X"/> <!-- BIPD -->
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="coverageFrom" type="types:NonNegativeInt">
                <xs:annotation>
                    <xs:documentation>
                        Indicates the lower limit of the coverage. Usually zero except for Excess 
                        BIPD insurance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="coverageTo" type="types:NonNegativeInt">
                <xs:annotation>
                    <xs:documentation>
                        Indicates the upper limit of the coverage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companyName" type="types:String50" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Insurance company name.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contact" type="types:String90" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Insurance company contact name.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="address" type="types:Address" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Insurance company address.<br/>
                        <br/>
                        Note that there are instances in which the registration data is incorrect 
                        and the postal code is not of the proper format for the designated country 
                        code.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="phone" type="types:PhoneNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Insurance company phone number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="fax" type="types:PhoneNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Insurance company fax number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
