    <xs:complexType name="AssetUpdate">
        <xs:annotation>
            <xs:documentation>
                Note that the updateable fields are all optional. Any field not specified for
                update will retain its current value. Once a field is set, its value can be
                changed, but there is no way to unset the field.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ltl" type="xs:boolean" minOccurs="0"/>
            <xs:element name="comments" type="types:String70" minOccurs="0" maxOccurs="2">
                <xs:annotation>
                    <xs:documentation>
                        The number of comments on a posting exposed to truckstops cannot be
                        decreased. Omitting comments from an update is allowable (the previous
                        comment(s) will be left unchanged), but including a single comment when 
                        the posting currently includes 2 will result in an error.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="count" type="tfmi:AssetCount" minOccurs="0"/>
            <xs:element name="dimensions" type="tfmi:Dimensions" minOccurs="0"/>
            <xs:element name="stops" type="types:NonNegativeInt" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
