            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="logoutRequest" type="tfmi:LogoutRequest" substitutionGroup="message:request"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LogoutOperation">
        <xs:annotation>
            <xs:documentation>
                This request is deprecated. Session tokens expire after 12 hours, so
                logout is
                unnecessary.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="types:Operation">
                <xs:sequence/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="logoutOperation" type="tfmi:LogoutOperation" substitutionGroup="types:operations"/>
    <!-- -->
    <!-- -->
    <!-- ====================================================================================== -->
