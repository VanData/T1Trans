    <xs:complexType name="DotAuthority">
        <xs:sequence>
            <xs:element name="commonAuthority" type="tfmi:AuthorityStatus">
                <xs:annotation>
                    <xs:documentation>
                        Status of this carrier's common authority.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contractAuthority" type="tfmi:AuthorityStatus">
                <xs:annotation>
                    <xs:documentation>
                        Status of this carrier's contract authority.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="brokerAuthority" type="tfmi:AuthorityStatus">
                <xs:annotation>
                    <xs:documentation>
                        Status of this carrier's broker authority.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="pendingCommonAuthority" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if this carrier has common authority
                        pending.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="pendingContractAuthority" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if this carrier has contract authority
                        pending.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="pendingBrokerAuthority" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if this carrier has broker authority
                        pending.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="revokedCommonAuthority" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if this carrier is on 30-day notice for revocation. This occurs 30 
                        days prior to the expiration of the carrier's BIPD and/or Cargo insurance 
                        and continues until proof of new coverage has been received by the FMCSA. 
                        It also occurs if the carrier's insurance is about to be canceled.<br/>
                        <br/>
                        Note: the carrier is still legal and authorized during this period.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="revokedContractAuthority" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if this carrier is on 30-day notice for revocation. This occurs 30 
                        days prior to the expiration of the carrier's BIPD and/or Cargo insurance 
                        and continues until proof of new coverage has been received by the FMCSA. 
                        It also occurs if the carrier's insurance is about to be canceled.<br/>
                        <br/>
                        Note: the carrier is still legal and authorized during this period.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="revokedBrokerAuthority" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if this carrier is on 30-day notice for revocation. This occurs 30 
                        days prior to the expiration of the carrier's BIPD and/or Cargo insurance 
                        and continues until proof of new coverage has been received by the FMCSA. 
                        It also occurs if the carrier's insurance is about to be canceled.<br/>
                        <br/>
                        Note: the carrier is still legal and authorized during this period.<br/>
                        <br/>
                        If this element is not included, no authority record was retrieved.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="freightFlag" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not the carrier is authorized for freight.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="passengerFlag" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not the carrier is authorized for passengers.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="hhgFlag" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not the carrier is authorized for household goods.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bipdRequired" type="types:NonNegativeInt">
                <xs:annotation>
                    <xs:documentation>
                        Numeric field indicating the amount of BIPD coverage this carrier is 
                        required to have.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cargoRequired" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether this carrier is required to file form BMC-34 showing
                        evidence of cargo coverage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bondSuretyRequired" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether this carrier is required to file form BMC-84 or 85
                        showing evidence of a broker bond or equivalent trust fund.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bipdOnFile" type="types:NonNegativeInt">
                <xs:annotation>
                    <xs:documentation>
                        The amount of BIPD coverage on file. Coverage details will be available
                        in the active/pending insurance section.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cargoOnFile" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether this carrier has filed form BMC-34 showing evidence of
                        cargo coverage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bondOnFile" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether this carrier has also filed form BMC-84 or 85 showing
                        evidence of a broker bond or trust fund.<br/>
                        <br/>
                        If true, coverage details will be in the active/pending insurance section.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
