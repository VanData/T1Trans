                            <xs:documentation>
                                Specifies whether the response should include CSA 2010 basic
                                measurement data.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="includeDotProfile" type="xs:boolean" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Specifies whether the response should include the carrier's
                                DOT Profile.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
