                            <xs:documentation>
                                Primary contact at the carrier.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="carrierPhoneNumber" type="types:PhoneNumber">
                        <xs:annotation>
                            <xs:documentation>
                                Carrier's contact phone number.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="carrierFaxNumber" type="types:PhoneNumber" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Carrier's contact fax number.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="comments" type="types:String512" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Any additional pertinent information for the TransCore 
                                representative.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
