    </xs:simpleType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="PostingCallback">
        <xs:annotation>
            <xs:documentation>
                Callback contact information supplied on lead takes.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="userId" type="types:UserId">
                <xs:annotation>
                    <xs:documentation>
                        The ID of the user that posted this asset.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:annotation>
                    <xs:documentation>
                        How to contact the poster.
                    </xs:documentation>
                </xs:annotation>
                <xs:element name="phone" type="tfmi:CallbackPhoneNumber"/>
                <xs:element name="email" type="tfmi:CallbackEmailAddress"/>
            </xs:choice>
            <xs:element name="name" type="types:PersonName">
                <xs:annotation>
                    <xs:documentation>
                        The first and last name of the poster.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companyName" type="types:String50">
                <xs:annotation>
                    <xs:documentation>
                        The full 50-char name of the company to which the poster belongs.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="displayCompany" type="types:TSNDisplayName">
                <xs:annotation>
                    <xs:documentation>
                        The short 8-char name of the company to which the poster belongs.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="postersStateProvince" type="types:StateProvince">
                <xs:annotation>
                    <xs:documentation>
                        The state/province in which the poster resides.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
