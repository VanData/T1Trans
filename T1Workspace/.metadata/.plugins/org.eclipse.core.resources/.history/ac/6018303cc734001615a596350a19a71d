                <xs:sequence>
                    <xs:element ref="tfmi:postAssetResults" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="postAssetResponse" type="tfmi:PostAssetResponse" substitutionGroup="message:response"/>
