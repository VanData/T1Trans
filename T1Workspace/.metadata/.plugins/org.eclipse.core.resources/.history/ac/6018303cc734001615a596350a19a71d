        <xs:complexContent>
            <xs:extension base="types:SuccessData">
                <xs:sequence>
                    <xs:element name="searches" type="tfmi:Search" minOccurs="0" maxOccurs="1000">
                        <xs:annotation>
                            <xs:documentation>
                                Details of the looked-up searches. 
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="unreported" type="tfmi:SearchId" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                In order to keep the payload size (and transmission time) 
                                reasonable, the number of searches returned is limited. For 
                                requesters with more searches, this element contains the IDs of
                                those searches not reported in "searches".<br/>
                                <br/>
                                Clients can then feed this element to a new LookupSearchesRequest,
                                using QuerySearchesBySearchIds, repeating as necessary until
                                "unreported" is no longer returned.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
