    <xs:complexType name="DotProfile">
        <xs:sequence>
            <xs:element name="duns" type="types:NonNegativeInt" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        DUNS Number
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="powerUnits" type="types:NonNegativeInt" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The number of power units operated by the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companyDrivers" type="types:NonNegativeInt" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The number of drivers employed by the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contractDrivers" type="types:NonNegativeInt" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        The number of drivers contracted by the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="commodities" type="tfmi:CommodityType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Commodities handled by the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="specialCommodities" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Free form text containing a description of commodities not covered in the 
                        standard DOT commodity list.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="operatingStates" type="types:StateProvince" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        States in which the carrier operates.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
