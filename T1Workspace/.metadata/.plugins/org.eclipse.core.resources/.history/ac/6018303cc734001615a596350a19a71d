    <!-- -->
    <xs:complexType name="LogoutResponse">
        <xs:complexContent>
            <xs:extension base="message:Response">
                <xs:sequence>
                    <xs:element ref="tfmi:logoutResult"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="logoutResponse" type="tfmi:LogoutResponse" substitutionGroup="message:response"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LogoutResult">
        <xs:complexContent>
            <xs:extension base="types:Result">
                <xs:sequence>
                    <xs:choice>
                        <xs:element ref="tfmi:logoutSuccessData"/>
                        <xs:element ref="types:serviceError"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="logoutResult" type="tfmi:LogoutResult" substitutionGroup="types:results"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LogoutSuccessData">
        <xs:complexContent>
            <xs:extension base="types:SuccessData">
                <xs:sequence>
                    <xs:element name="Result" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>
                                Always returns true.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="logoutSuccessData" type="tfmi:LogoutSuccessData" substitutionGroup="types:data"/>
