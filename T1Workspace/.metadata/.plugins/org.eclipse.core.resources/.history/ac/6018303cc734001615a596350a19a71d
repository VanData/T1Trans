    <xs:complexType name="DeleteAlarmRequest">
        <xs:annotation>
            <xs:documentation>
                Delete an alarm.<br/>
                <br/>
                To "delete" means the alarm is no longer available for freight matching activities
                and is no longer retrieved on multi-alarm lookups.<br/>
                <br/>
                If the alarm is currently active (i.e., not expired) then it will be automatically
                cancelled (and an alarm termination notice will be issued).<br/>
                <br/>
                It is not a requirement that alarm ever be explicitly deleted. Deletion is a 
                convenience for applications that make heavy use of multi-alarm lookups (e.g.,
                <code>LookupAlarmOperation.LookupAllMyAlarms</code>).
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="message:Request">
                <xs:sequence>
                    <xs:element ref="tfmi:deleteAlarmOperation"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="deleteAlarmRequest" type="tfmi:DeleteAlarmRequest" substitutionGroup="message:request"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="DeleteAlarmOperation">
        <xs:complexContent>
            <xs:extension base="types:Operation">
                <xs:sequence>
                    <xs:choice>
                        <xs:element ref="tfmi:deleteAlarmsByAlarmIds">
                            <xs:annotation>
                                <xs:documentation>
                                    Delete one or more alarms by AlarmId.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="tfmi:deleteAllMyAlarms">
                            <xs:annotation>
                                <xs:documentation>
                                    Delete all alarms owned by the requester.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="tfmi:deleteAllMyGroupsAlarms">
                            <xs:annotation>
                                <xs:documentation>
                                    Delete all alarms belonging by the requester or to other member's 
                                    of the requester's sharing group.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
