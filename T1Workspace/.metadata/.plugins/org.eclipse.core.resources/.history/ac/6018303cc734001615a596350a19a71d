<?xml version="1.0" encoding="UTF-8"?><!-- $Id: TfmiFreightMatching.xsd 18346 2014-07-28 18:43:44Z chriso $ --><!-- Best viewed with fixed-width (e.g., Courier) font --><xs:schema xmlns:tfmi="http://www.tcore.com/TfmiFreightMatching.xsd" xmlns:tfmiwsdl="http://www.tcore.com/TfmiFreightMatching.wsdl" xmlns:tfmirates="http://www.tcore.com/TfmiRates.xsd" xmlns:headers="http://www.tcore.com/TcoreHeaders.xsd" xmlns:message="http://www.tcore.com/TcoreMessage.xsd" xmlns:types="http://www.tcore.com/TcoreTypes.xsd" xmlns:cwtypes="http://www.tcore.com/TcoreCarrierWatchTypes.xsd" xmlns:dobtypes="http://www.tcore.com/TcoreDobTypes.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.tcore.com/TfmiFreightMatching.xsd" elementFormDefault="qualified">

    <xs:annotation>
        <xs:documentation>
            <h2>TransCore Freight Matching Interface Schema</h2><br/>
            <br/>
            This schema defines a simple post/search external API to the
            TransCore Freight Matching
            Service<br/>
            <br/>
            Freight matching is the process of bringing together organizations
            who have freight
            that
            needs to be moved, with those who have the
            ability to move their
            freight.<br/>
            <br/>
            The primary functionality provided by the freight matching service
            is:
            <ul>
                <li>The ability to post assets (loads and trucks)</li>
                <li>The ability to search for assets, based on a variety of
                    criteria.
                    Searches provide
                    'real-time' snapshots of availability.</li>
                <li>The ability to set an 'alarm' - a forward looking search that
                    will
                    alert the user
                    in real-time of assets matching their criteria.</li>
            </ul><br/>
            <br/>
            Published: March 2012
            <br/>
            Version: tfmiFreightMatching-2.0<br/>
            <br/>
            <i>Copyright (C) 2004-2013, TransCore LP, All Rights Reserved.</i><br/>
            <br/>
            <br/>
        </xs:documentation>
        <xs:appinfo>
            <wsdl>
                <service name="TfmiFreightMatching" namespace="tfmi">
                    <operation name="Login"/>
                    <!-- <operation name="Logout" /> --> <!-- Logout is still included in schema, but deprecated and removed from WSDL -->
                    <operation name="PostAsset"/>
                    <operation name="UpdateAsset"/>
                    <operation name="DeleteAsset"/>
                    <operation name="LookupAsset"/>
                    <operation name="LookupAlarm"/>
                    <operation name="CreateAlarm"/>
                    <operation name="UpdateAlarm"/>
                    <operation name="DeleteAlarm"/>
                    <operation name="CreateSearch"/>
                    <operation name="LookupSearch"/>
                    <operation name="LookupSearchMatches"/>
                    <operation name="CountAssets"/>
                    <operation name="DeleteSearch"/>
                    <operation name="LookupAlarmUrl"/>
                    <operation name="UpdateAlarmUrl"/>
                    <!-- <operation name="LookupCarrierInfo" /> --> <!-- LookupCarrierInfo is still included in schema, but deprecated and removed from WSDL -->
                    <operation name="LookupCarrier"/>
                    <operation name="MonitorInsurance"/>
                    <operation name="CreateCarrier"/>
                    <operation name="LookupDobSignedCarriers"/>
                    <operation name="LookupDobCarriers"/>
                    <operation name="lookupDobEvents"/>
                    <operation name="LookupCapabilities"/>
                </service>

                <!-- Rates requests are included here for WSDL generation -->
                <service name="TfmiRates" namespace="tfmirates">
                    <operation name="LookupRate"/>
                    <operation name="LookupHistoricSpotRates"/>
                    <operation name="LookupHistoricContractRates"/>
                    <operation name="LookupDoeFuelPrices"/>
                </service>
            </wsdl>
        </xs:appinfo>
    </xs:annotation>
