                <xs:sequence>
                    <xs:element name="alarmId" type="tfmi:AlarmId">
                        <xs:annotation>
                            <xs:documentation>
                                The ID of this alarm. This ID must be used when cancelling, 
                                updating, or querying this alarm.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="alarm" type="tfmi:Alarm">
                        <xs:annotation>
                            <xs:documentation>
                                A copy of the alarm that was created. This potentially saves the
                                client a LookupAlarm request.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="matches" type="tfmi:MatchingAsset" minOccurs="0" maxOccurs="300">
                        <xs:annotation>
                            <xs:documentation>
                                If the alarm requested an initial lookback search, this is the list
                                of matches from that search. If no matches were found, this list is
                                empty.<br/>
                                <br/>
                                If the alarm did not request an initial lookback search, this 
                                element is omitted.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
