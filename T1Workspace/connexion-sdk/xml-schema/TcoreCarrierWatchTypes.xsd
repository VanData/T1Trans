    <xs:complexType name="Csa2010BasicMeasurements">
        <xs:annotation>
            <xs:documentation>
                Provides CSA 2010 Basic Measurements
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdateOfCsa2010Basics" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>
                        The last date on which the Csa 2010 Basic Measurements was updated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="insuranceOrOtherViolation" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Insurance/Other BASIC Serious Violation Indicator (true = Serious Violation from investigation within previous 12 months)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="insuranceOrOtherIndicator" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Insurance/Other BASIC Indicator (true = Serious Violation within previous 12 months)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="measurement" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        The basic carrier safety measurements.
