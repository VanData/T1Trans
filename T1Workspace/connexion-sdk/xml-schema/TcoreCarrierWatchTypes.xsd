    <xs:complexType name="Header">
        <xs:sequence>
            <xs:element name="tcoreId" type="xs:long" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        TransCore's own carrier identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalName" type="types:String155" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Best available legal business name. 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dbaName" type="types:String155" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Best available commonly used business name.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dotNumber" type="types:NonNegativeInt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        DOT Identification Number of the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="docket" type="cwtypes:DocketType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="intrastate" type="cwtypes:IntrastateType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="duns" type="types:NonNegativeInt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        DUNS Number, when available.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="scac" type="types:String4" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        When available, the Standard Carrier Alpha Code (SCAC) is a unique code used to identify transportation companies.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="physicalLocation" type="cwtypes:Location" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The best and last available physical address.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mailingLocation" type="cwtypes:Location" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The best and last available mailing address.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
