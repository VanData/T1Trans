    <xs:complexType name="InsuranceCertificate">
        <xs:annotation>
            <xs:documentation>
                Describes an insurance certificate 
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="coverageCategory" type="types:String20" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The type of category this certificate falls under: CARGO, AUTO, GENERAL, WORKERS_COMP, or OTHER.  
                        OTHER may occur multiple times, but the others will occur only once.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="coverageType" type="types:String90" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The full name provided by the insurance company that describes the type of coverage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="effectiveDate" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The date on which this policy went into or is going into effect.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="expirationDate" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The date on which this policy will expire. 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="canceledDate" type="xs:date" minOccurs="0" maxOccurs="1">
