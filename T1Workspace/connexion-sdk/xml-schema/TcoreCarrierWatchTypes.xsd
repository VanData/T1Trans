    <xs:complexType name="DotProfile">
        <xs:annotation>
            <xs:documentation>
                DOT Profile of the carrier queried.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdateOfDotProfile" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date on which the DOT updated their profile for this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="isActive" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not the carrier is still operating or not.  If not, 
                        no other data will be provided in this section.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="entityType" type="types:String30" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The DOT classification for the type of company this is.<br/>
                        <ul>
                        <li>Carrier</li>
                        <li>Shipper</li>
                        <li>Registrant</li>
                        <li>Carrier &amp; Shipper</li>
                        </ul>
                        Abnormal values may be:
                        <ul>
                        <li>Intermodal Equipment Provider</li>
                        <li>New Entrant Status</li>
                        </ul>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="operationType" type="types:String25" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Identifies the carrier as being engaged in interstate, intrastate hazardous 
                        material, or intrastate non-hazardous material transport activities.<br/>
                        <ul>
                        <li>Interstate</li> 
                        <li>Intrastate Only (HM)</li>
                        <li>Intrastate Only (Non-HM)</li>
                        </ul>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="outOfInterstateServiceDate" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The date the carrier went out of Interstate service.  Only provided if the carrier is out of service.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
                        
            <xs:element name="powerUnits" type="types:NonNegativeInt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The number of power units operated by the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="drivers" type="types:NonNegativeInt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The number of drivers employed and contracted by the carrier. 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mcs150Mileage" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="mileageYear" type="types:NonNegativeInt" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    Mileage year from the MCS-150 Registration Form.  
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="formDate" type="xs:date" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    Date from the MCS-150 Registration Form. 
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="mileage" type="xs:long" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    Mileage from the MCS-150 Registration Form. 
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="commodities" type="cwtypes:CommodityType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Commodities handled by the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="specialCommodities" type="types:String256" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Free form text containing a description of commodities not covered in 
                        the standard DOT commodity list.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>  
