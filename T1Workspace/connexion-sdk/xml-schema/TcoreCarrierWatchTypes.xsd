    <xs:complexType name="DotAuthority">
        <xs:annotation>
            <xs:documentation>
                Provides DOT authority and insurance requirements.  
                Only included if the DOT Authority is selected for download and the carrier has a known docket number.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdateOfDotAuthority" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date on which the DOT updated their authority information for this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="commonAuthority" type="cwtypes:AuthorityStatus" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Common authority for the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="pendingCommonAuthority" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not common authority is pending.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="revokedCommonAuthority" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not common authority may be revoked in the near future.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>         
            <xs:element name="contractAuthority" type="cwtypes:AuthorityStatus" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Contract authority for the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="pendingContractAuthority" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not contract authority is pending.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="revokedContractAuthority" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not contract authority may be revoked in the near future.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="brokerAuthority" type="cwtypes:AuthorityStatus" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Broker authority for the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="pendingBrokerAuthority" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not broker authority is pending.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="revokedBrokerAuthority" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not broker authority may be revoked in the near future.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="carriesFreight" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not the carrier is authorized for freight.<br/> 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="carriesPassengers" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not the carrier is authorized to carry passengers. 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="carriesHhg" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether or not the carrier is authorized to carry 
                        household goods. 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bipdRequired" type="types:NonNegativeInt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The amount of BIPD coverage this carrier is required to have.<br/> 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cargoRequired" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether this carrier is required to file form BMC-34 showing 
                        evidence of cargo coverage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bondSuretyRequired" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether this carrier is required to file form BMC-84 or 85 
                        showing evidence of a broker bond or equivalent trust fund.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bipdOnFile" type="types:NonNegativeInt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The amount of BIPD coverage on file. Coverage details will be available 
                        in the active/pending insurance section.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cargoOnFile" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether this carrier has filed form BMC-34 showing evidence of 
                        cargo coverage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bondOnFile" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether this carrier has also filed form BMC-84 or 85 showing 
                        evidence of a broker bond or trust fund.<br/>
                        <br/>
                        If true, coverage details will be in the active/pending insurance 
                        section.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
