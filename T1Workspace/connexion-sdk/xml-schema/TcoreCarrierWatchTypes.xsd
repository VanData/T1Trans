    <xs:complexType name="InsuranceCertificates">
        <xs:annotation>
            <xs:documentation>
                Describes a certification of insurance carried by the carrier.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="lastUpdateOfInsuranceCertificates" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The last date on which the insurance certificate for this carrier was updated
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="maxCargoLimit" type="types:Currency" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The maximum coverage limit in the cargo insurance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="coverage" type="cwtypes:InsuranceCertificate" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Describes cargo coverage carried by the carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
