    <xs:complexType name="IntrastateType">
        <xs:sequence>
            <xs:element name="intrastateState" type="types:StateProvince" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The state the carrier is authorized to operate within.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="intrastateCode" type="types:String15" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The state's assigned operating code for this carrier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>  

