    <xs:complexType name="InsuranceRecord">
        <xs:annotation>
            <xs:documentation>
                DOT provided insurance data on the company.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="coverageType" type="cwtypes:InsuranceType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        BIPD, Cargo, Trust, or Bond
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="bipdClass" type="cwtypes:InsuranceClass" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        If insuranceType = BIPD this element indicates whether the following 
                        coverage details are for primary coverage or excess coverage.<br/>
                        <br/>
                        For other insurance types, this element will be omitted.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="formCode" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        34 = Cargo<br/>
                        91,91X = BIPD<br/>
                        82 = BIPD (Surety Bond)<br/>
                        83 = Cargo (Surety Bond)<br/>
                        84 = Property Brokers Surety Bond<br/>
                        85 = Property Brokers Trust Fund Agreement<br/>
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="types:String3">
                        <xs:enumeration value="34"/> <!-- Cargo -->
                        <xs:enumeration value="82"/> <!-- BIPD (Surety Bond) -->
                        <xs:enumeration value="83"/> <!-- Cargo (Surety Bond)-->
                        <xs:enumeration value="84"/> <!-- Property Brokers Surety Bond -->
                        <xs:enumeration value="85"/> <!-- Property Brokers Trust Fund Agreement -->
                        <xs:enumeration value="91"/> <!-- BIPD -->
                        <xs:enumeration value="91X"/> <!-- BIPD -->
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
             <xs:element name="policyNumber" type="types:String40" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The insurance policy number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="coverageFrom" type="types:NonNegativeInt" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates the lower limit of the coverage. Usually zero except for 
                        Excess BIPD insurance. 
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="coverageTo" type="types:NonNegativeInt" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Indicates the upper limit of the coverage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="effectiveDate" type="xs:date" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Date this insurance policy became effective.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="canceledDate" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        Date this insurance policy was canceled.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="insuranceCarrier" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The contact information for the insurance carrier.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="id" type="types:NonNegativeInt" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    A unique identifier provided by Volpe.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="companyName" type="types:String55" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    Insurance company name.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="contact" type="types:String55" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    Insurance company contact name.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="location" type="cwtypes:Location" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    Insurance company address.
                                    <br/>
                                    Note that there are instances of incorrect registration data, in which 
                                    the postal code is not of the proper format for the designated country
                                    code.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                   </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
