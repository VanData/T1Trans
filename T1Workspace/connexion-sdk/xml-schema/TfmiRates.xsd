    <xs:complexType name="LookupHistoricRatesOperation">
        <xs:complexContent>
            <xs:extension base="types:Operation">
                <xs:sequence>
                    <xs:element name="equipment" type="tfmirates:RateEquipmentCategory">
                        <xs:annotation>
                            <xs:documentation>
                                Rates deal with broader groups of equipment than EquipmentTypes or 
                                EquipmentClasses used in Freight Matching, so the set of equipment
                                categories is much smaller. 
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="origin" type="tfmi:Place">
                        <xs:annotation>
                            <xs:documentation>
                                The origin of the lane.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="destination" type="tfmi:Place">
                        <xs:annotation>
                            <xs:documentation>
                                The destination of the lane. 
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
