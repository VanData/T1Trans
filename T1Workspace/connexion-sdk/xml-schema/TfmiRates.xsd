                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="destinationRamp" type="xs:boolean" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                ??? This field is in the DB, but not in the Excel formats
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
