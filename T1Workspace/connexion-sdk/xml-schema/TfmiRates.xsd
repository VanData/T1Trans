    <xs:complexType name="LookupHistoricSpotRatesRequest">
        <xs:annotation>
            <xs:documentation>
                Requests spot rate estimates for a lane over the past 13 months. Each rate 
                returned will be credited against the pool of lookups associated with the 
                user's Truckload Rate Index Subscription.   
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="message:Request">
                <xs:sequence>
                    <xs:element name="lookupHistoricSpotRatesOperation" type="tfmirates:LookupHistoricSpotRatesOperation"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupHistoricSpotRatesRequest" type="tfmirates:LookupHistoricSpotRatesRequest" substitutionGroup="message:request"/>
