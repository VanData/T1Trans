    <xs:complexType name="LookupDoeFuelPricesRequest">
        <xs:annotation>
            <xs:documentation>
                Lookup diesel fuel prices over the past 13 months, as reported by the 
                Department of Energy.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="message:Request">
                <xs:sequence>
                    <xs:element name="lookupDoeFuelPricesOperation" type="tfmirates:LookupDoeFuelPricesOperation"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupDoeFuelPricesRequest" type="tfmirates:LookupDoeFuelPricesRequest" substitutionGroup="message:request"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LookupDoeFuelPricesOperation">
        <xs:complexContent>
            <xs:extension base="types:Operation">
                <xs:sequence/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupDoeFuelPricesOperation" type="tfmirates:LookupDoeFuelPricesOperation" substitutionGroup="types:operations"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LookupDoeFuelPricesResponse">
        <xs:complexContent>
            <xs:extension base="message:Response">
                <xs:sequence>
                    <xs:element ref="tfmirates:lookupDoeFuelPricesResult" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupDoeFuelPricesResponse" type="tfmirates:LookupDoeFuelPricesResponse" substitutionGroup="message:response"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LookupDoeFuelPricesResult">
        <xs:complexContent>
            <xs:extension base="types:Result">
                <xs:sequence>
                    <xs:choice>
                        <xs:element ref="tfmirates:lookupDoeFuelPricesSuccessData"/>
                        <xs:element ref="types:serviceError"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupDoeFuelPricesResult" type="tfmirates:LookupDoeFuelPricesResult" substitutionGroup="types:results"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LookupDoeFuelPricesSuccessData">
        <xs:complexContent>
            <xs:extension base="types:SuccessData">
                <xs:sequence>
                    <xs:element name="monthlyReports" type="tfmirates:DoeFuelPriceMonthlyReport" maxOccurs="unbounded">
				        <xs:annotation>
				            <xs:documentation>
				                The DOE reports prices for the current month a few days into the 
                                month. If a price is available for the current month, the report 
                                will include the current month and the previous 12 months. If no 
                                price is available for the current month, the report will instead 
                                include the previous 13 months. 
				            </xs:documentation>
				        </xs:annotation>
				    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
