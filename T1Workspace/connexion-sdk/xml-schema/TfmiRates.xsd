    <xs:complexType name="SubmitRatesRequest">
        <xs:annotation>
            <xs:documentation>
                Submit rate information to TransCore. 
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="message:Request">
                <xs:sequence>
                    <xs:element name="submitRatesOperation" type="tfmirates:SubmitRatesOperation"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="submitRatesRequest" type="tfmirates:SubmitRatesRequest" substitutionGroup="message:request"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="SubmitRatesOperation">
        <xs:complexContent>
            <xs:extension base="types:Operation">
                <xs:sequence>
                    <xs:element name="contributorType">
                        <!-- Note: In the database, 1=Broker 2=Carrier 3=Intermodal 4=Shipper 5=Contract Carrier -->
                        <xs:annotation>
                            <xs:documentation>
                                Identifies the class of contributor.<br/>
                                <br/>
                                Do we want to require this on every submission? 
                                Or record it at the company or office level?    
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Broker"/>
                                <xs:enumeration value="Carrier"/>
                                <xs:enumeration value="ContractCarrier"/>
                                <xs:enumeration value="Shipper"/>
                            </xs:restriction>        
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="loadId" type="types:String25">
                        <xs:annotation>
                            <xs:documentation>
                                Uniquely identifies the specific load. If the rate or details of 
                                a load changes after initial submission, this identifier will be 
                                used to update the original submission.    
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="pickupDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>
                                Date the load was or will be picked up.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="origin" type="tfmi:Place">
                        <xs:annotation>
                            <xs:documentation>
                                Selected using one of the standard Connexion location-specification
                                methods.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="destination" type="tfmi:Place">
                        <xs:annotation>
                            <xs:documentation>
                                Selected using one of the standard Connexion location-specification
                                methods.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="totalAmountPaid">
                           <xs:annotation>
                               <xs:documentation>
                                   The total amount paid to the carrier.<br/>
                                   <br/>
                                   Note: It looks like shipper contributions populate this column 
                                   in the DB (rather than TOTAL_AMNT_PD_SHIPPER)
                               </xs:documentation>
                           </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="0"/>
                                <xs:maxInclusive value="99999.99"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="totalAmountPaidByShipper" minOccurs="0"> <!-- Only for Broker or Shipper -->
                           <xs:annotation>
                               <xs:documentation>
                                   The total amount paid by the shipper, if different than the 
                                   amount paid to the carrier (e.g., if the shipment was brokered). 
                                   If present, this field should generally be equal to or greater 
                                   than 'totalAmountPaid'.
                               </xs:documentation>
                           </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="0"/>
                                <xs:maxInclusive value="99999.99"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="equipment" type="types:EquipmentType">
	                    <xs:annotation>
	                        <xs:documentation>
	                            Standard TransCore EquipmentType code   
	                        </xs:documentation>
	                    </xs:annotation>
                    </xs:element>
                    <xs:element name="equipmentDetail" type="types:String50" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                A more detailed description of the equipment than that specified 
                                in 'equipment', if available. e.g. 'Air Ride', 'Goose Neck', 
                                'Dbl Drop 53 ft', etc. 
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="mode" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                The mode of transport. If unspecified, defaults to 'Truckload'.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Truckload"/>
                                <xs:enumeration value="LTL"/>
                                <xs:enumeration value="Parcel"/>
                                <xs:enumeration value="Air"/>
                                <xs:enumeration value="Ocean"/>
                                <xs:enumeration value="Rail"/>
                                <xs:enumeration value="Overweight"/>
                                <xs:enumeration value="Straight Truck"/>
                                <xs:enumeration value="Intermodal"/>
                            </xs:restriction>        
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="miles" type="types:NonNegativeInt" minOccurs="0"> 
                        <xs:annotation>
                            <xs:documentation>
                                Total mileage.<br/>
                                <br/>
                                Note: I'm told we substitute our own CSB mileage anyway
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>                        
                    <xs:sequence minOccurs="0">
                        <xs:element name="pieces" type="types:NonNegativeInt">
	                        <xs:annotation>
	                            <xs:documentation>
	                                Number of individual Pallets, Crates, Barrels, etc. in the 
	                                shipment.<br/>
                                    <br/>
                                    'pieces' and 'units' are not required, but if 'pieces' is 
                                    specified, 'units' must be as well, describing what that count 
                                    represents.<br/>
	                                <br/>
	                                Note: In Broker format, there is a '# of Units' field that 
	                                looks to be equivalent.
	                            </xs:documentation>
	                        </xs:annotation>
                        </xs:element>
                        <xs:element name="units">
                            <xs:annotation>
                                <xs:documentation>
                                    Describes the 'pieces' field.<br/>
                                    <br/>
                                    Pieces and units are not required, but if pieces is specified, 
                                    units must be as well, describing what that count 
                                    represents.<br/>
                                    <br/>
                                    Note: In Broker format, there is a '# of Units' field that 
                                    looks to be equivalent.
                                </xs:documentation>
                            </xs:annotation>
	                        <xs:simpleType>
	                            <xs:restriction base="xs:string">
	                                <xs:enumeration value="Pallets"/>
	                                <xs:enumeration value="Crates"/>
	                                <xs:enumeration value="Barrels"/>
	                                <xs:enumeration value="Other"/>
	                            </xs:restriction>        
	                        </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                    <xs:element name="weightPounds" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Total shipment weight in pounds   
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:int">
                                <xs:minInclusive value="1"/>
                                <xs:maxInclusive value="199999"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="freightClass" type="types:String50" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                ???<br/>
                                <br/>
                                For LTL loads, one or more of 'freightClass', 'cubicFeet', and 
                                'linearFeet' are required.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="cubicFeet" type="types:NonNegativeInt" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Total shipment volume in cubic feet
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="linearFeet" type="types:NonNegativeInt" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Shipment length in feet
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="pickups" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Number of separate pickups for this trip, including 
                                origin. 
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:int">
                                <xs:minInclusive value="1"/>
                                <xs:maxInclusive value="99"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="drops" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Number of separate drops for this trip, including 
                                destination. 
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:int">
                                <xs:minInclusive value="1"/>
                                <xs:maxInclusive value="99"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="linehaulAmount" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Total linehaul paid for this trip in US Dollars, less all 
                                accessorials and fuel charges.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="0"/>
                                <xs:maxInclusive value="99999.99"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="fuelSurchargeAmount" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Fuel surcharge paid for this trip in US Dollars.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="0"/>
                                <xs:maxInclusive value="99999.99"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="loadUnloadAmount" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Total load/unload charges paid for this trip in US Dollars.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="0"/>
                                <xs:maxInclusive value="99999.99"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="stopChargeAmount" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Total charges for split pickup and/or stop-off in US Dollars.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="0"/>
                                <xs:maxInclusive value="99999.99"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="otherAccessorialCharge" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Other accessorial charges paid for this trip, including 
                                description.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="amount">
			                        <xs:annotation>
			                            <xs:documentation>
			                                 Total of all other accessorial charge amounts in 
			                                 US Dollars.  
			                            </xs:documentation>
			                        </xs:annotation>
			                        <xs:simpleType>
			                            <xs:restriction base="xs:float">
			                                <xs:minInclusive value="0"/>
			                                <xs:maxInclusive value="99999.99"/>
			                            </xs:restriction>
			                        </xs:simpleType>
                                </xs:element>
                                <xs:element name="description" type="types:String50">
                                    <xs:annotation>
                                        <xs:documentation>
                                             Descripton of other accessorial charge(s).  
                                        </xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="rateType">
                        <xs:annotation>
                            <xs:documentation>
                                I'm probably misinterpreting, but it looks like this is 
                                alternatively titled 'Brokered Frt Flag' and 
                                'Price set on spot market basis' Do the two fields mean the same 
                                thing (it appears they're both stored into the BR_FREIGHT_FLAG 
                                column). Should we offer a choice between two field names? 
                                Is it required? If not, will we default to one or the other 
                                when not included?
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
					        <xs:restriction base="xs:string">
					            <xs:enumeration value="Spot"/>
					            <xs:enumeration value="Contract"/>
					        </xs:restriction>        
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="commodity" type="types:String50" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Text description of the commodity carried.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="serviceLevel" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Regular"/>
                                <xs:enumeration value="Premium"/>
                                <xs:enumeration value="Expedited"/>
                                <xs:enumeration value="Team"/>
                            </xs:restriction>        
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="deliveryDate" type="xs:date" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                ??? This field is in the DB, but not in the Excel formats.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="deliveryApptRequired" type="xs:boolean" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                True if a delivery appointment is required for this load.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="deliveryApptNote" type="types:String40" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                If a delivery appointment is required, optionally includes 
                                additional information about the appointment   
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                                        
                    <xs:element name="firstDeliveryApptTime" type="xs:time" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Start time of the delivery appointment (or, the first delivery 
                                appointment if more than one was required).   
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="shipperName" type="types:String100" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Name of the shipper, if available.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="consigneeName" type="types:String100" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Name of the consignee (if any).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- Not for broker format -->
                    <xs:element name="brokerName" type="types:String100" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Name of the broker (if any).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="freightValue" type="types:NonNegativeInt" minOccurs="0"> <!-- Range? float or int?-->
                        <xs:annotation>
                            <xs:documentation>
                                Value of the freight carried in US Dollars.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="cargoInsurance" type="types:NonNegativeInt" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Amount of cargo insurance carried by the carrier in US Dollars.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="railroad" type="types:String100" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                ??? What do we want here? Name of the railroad?<br/>
                                <br/>
                                Only valid for shipper contributions
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="carrierName" type="types:String100" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                Name of the carrier (unnecessary for rate submissions by the 
                                carrier)   
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="scacCode" type="types:String4" minOccurs="0"> <!-- Only for shipper and broker formats. Is there any reason this should be longer than 4 characters?-->
                        <xs:annotation>
                            <xs:documentation>
                                Standard Carrier Alpha Code of the carrier (unnecessary for rate 
                                submissions by the carrier)   
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="locationName" type="types:String50" minOccurs="0"> <!-- Not for broker-->
                        <xs:annotation>
                            <xs:documentation>
                                ??? Name of the pickup location? Delivery location?
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
