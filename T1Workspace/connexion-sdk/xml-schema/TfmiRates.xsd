    <xs:complexType name="LookupBilledRateTransactionsRequest">
        <xs:annotation>
            <xs:documentation>
                Looks up the rate transactions during current billing period for the
                user's subscription. Note that this assumes the user will have only a single
                rate subscription (spot, combo, etc.)
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="message:Request">
                <xs:sequence>
                    <xs:element ref="tfmirates:lookupBilledRateTransactionsOperation"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupBilledRateTransactionsRequest" type="tfmirates:LookupBilledRateTransactionsRequest" substitutionGroup="message:request"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LookupBilledRateTransactionsOperation">
        <xs:complexContent>
            <xs:extension base="types:Operation">
                <xs:sequence/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupBilledRateTransactionsOperation" type="tfmirates:LookupBilledRateTransactionsOperation" substitutionGroup="types:operations"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LookupBilledRateTransactionsResponse">
        <xs:annotation>
            <xs:documentation>
                Summary and details of the rate lookup transactions for the user's subscription and
                contributions from the user's office during the current billing cycle. Any
                contribution credits will be applied to the next billing cycle (although not
                necessarily to the user's own subscription if the office maintains multiple rating
                subscriptions).
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="message:Response">
                <xs:sequence>
                    <xs:element ref="tfmirates:lookupBilledRateTransactionsResult" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupBilledRateTransactionsResponse" type="tfmirates:LookupBilledRateTransactionsResponse" substitutionGroup="message:response"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LookupBilledRateTransactionsResult">
        <xs:complexContent>
            <xs:extension base="types:Result">
                <xs:sequence>
                    <xs:choice>
                        <xs:element ref="tfmirates:lookupBilledRateTransactionsSuccessData"/>
                        <xs:element ref="types:serviceError"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupBilledRateTransactionsResult" type="tfmirates:LookupBilledRateTransactionsResult" substitutionGroup="types:results"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="LookupBilledRateTransactionsSuccessData">
        <xs:complexContent>
            <xs:extension base="types:SuccessData">
                <xs:sequence>
                    <xs:element name="lastUpdated" type="types:UTC">
                        <xs:annotation>
                            <xs:documentation>
                                The last update of the billing record. This will generally be
                                updated near real-time, but may not always include very recent 
                                lookups.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="lastBillDate" type="types:UTC">
                        <xs:annotation>
                            <xs:documentation>
                                The start of the current billing cycle.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="nextBillDate" type="types:UTC">
                        <xs:annotation>
                            <xs:documentation>
                                The end of the current billing cycle.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="productName" type="types:String100">
                        <xs:annotation>
                            <xs:documentation>
                                A brief description of the rate product (subscription) to which
                                the user is assigned. (e.g. 'Silver (10,000 lanes/month)')
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="lanesAllowed" type="types:NonNegativeInt">
                        <xs:annotation>
                            <xs:documentation>
                                The number of lane lookups allowed during the current billing
                                cycle.
                                <br/>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="lanesUsed" type="types:NonNegativeInt">
                        <xs:annotation>
                            <xs:documentation>
                                The number of lane lookups performed during the current billing
                                cycle.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="lanesCredited" type="types:NonNegativeInt">
                        <xs:annotation>
                            <xs:documentation>
                                The number of lane lookups credited for contributing lane rate
                                information during the current billing cycle.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="lanesRemaining" type="types:NonNegativeInt">
                        <xs:annotation>
                            <xs:documentation>
                                The number of lane lookups remaining during the current billing
                                cycle (This will always be equivalent to 
                                'lanesAllowed' - 'lanesUsed' + 'lanesCredited')
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="lookupDetails" type="tfmirates:BilledRateTransaction" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="contributionDetails" type="tfmirates:CreditedRateContribution" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <xs:element name="lookupBilledRateTransactionsSuccessData" type="tfmirates:LookupBilledRateTransactionsSuccessData" substitutionGroup="types:data"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="RateTransaction">
        <xs:sequence>
            <xs:element name="date" type="types:UTC">
                <xs:annotation>
                    <xs:documentation>
                        Date of the lookup transaction.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="laneCount" type="types:NonNegativeInt">
                <xs:annotation>
                    <xs:documentation>
                        Number of lanes looked up.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="BilledRateTransaction">
        <xs:complexContent>
            <xs:extension base="tfmirates:RateTransaction">
                <xs:sequence>
                    <xs:element name="rateType" type="tfmirates:RateType">
                        <xs:annotation>
                            <xs:documentation>
                                Lookup type (spot or contract).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="equipment" type="tfmirates:RateEquipmentCategory">
                        <xs:annotation>
                            <xs:documentation>
                                Equipment category queried in this lookup transaction.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="charge" type="xs:float">
                        <xs:annotation>
                            <xs:documentation>
                                Estimated charge for this transaction. This value will generally be 
                                zero until the contracted limit for the subscription is reached,
                                after which each lane looked up will be billed at the contracted 
                                overage rate.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="CreditedRateContribution">
        <xs:complexContent>
            <xs:extension base="tfmirates:RateTransaction">
                <xs:sequence>
                    <xs:element name="estimatedCredit" type="types:NonNegativeInt">
                        <xs:annotation>
                            <xs:documentation>
                                Estimate of the number of lane lookups which will be credited to 
                                the user's office during the next billing cycle as a result of 
                                this contribution.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- --> 
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:simpleType name="RateEquipmentCategory">
        <xs:annotation>
            <xs:documentation>
                Rates deal with broader groups of equipment than either EquipmentTypes or 
                EquipmentClasses. Each of the below categories includes one or more 
                EquipmentClasses.
                <br/>
                Note: These categories apply only to rate calculations. They do not apply to other
                TransCore requests (e.g. freight matching and carrier information).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Vans"/>
            <xs:enumeration value="Flatbeds"/>
            <xs:enumeration value="Reefers"/>
        </xs:restriction>        
    </xs:simpleType>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:simpleType name="RateType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Contract"/>
            <xs:enumeration value="Spot"/>
        </xs:restriction>
    </xs:simpleType>
