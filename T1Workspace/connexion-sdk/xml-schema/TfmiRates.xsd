    <xs:element name="lookupDoeFuelPricesSuccessData" type="tfmirates:LookupDoeFuelPricesSuccessData" substitutionGroup="types:data"/>
    <!-- -->
    <!-- -->
    <!-- -->
    <!-- -->
    <xs:complexType name="DoeFuelPriceMonthlyReport">
        <xs:sequence>
            <xs:element name="when" type="types:YearMonth">
		        <xs:annotation>
		            <xs:documentation>
		                The month and year represented by this report. 
		            </xs:documentation>
		        </xs:annotation>
            </xs:element>
            <xs:element name="pricePerGallon" type="xs:float">
		        <xs:annotation>
		            <xs:documentation>
		                Average monthly diesel price in USD. 
		            </xs:documentation>
		        </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
