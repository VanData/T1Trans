    <xs:complexType name="LookupHistoricSpotRatesResult">
        <xs:complexContent>
            <xs:extension base="types:Result">
                <xs:sequence>
                    <xs:choice>
                        <xs:element ref="tfmirates:lookupHistoricSpotRatesSuccessData"/>
                        <xs:element ref="types:serviceError"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
