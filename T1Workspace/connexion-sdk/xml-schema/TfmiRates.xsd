                            <xs:documentation>
                                ??? Date payment was due? Date it was received?
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="hazmat" type="xs:boolean" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>
                                True if the load is considered hazardous material.<br/>
                                <br/>
                                Note: This field is in the Excel formats, but doesn't appear to be 
